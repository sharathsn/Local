<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OCE__Can_not_enter_negative_numbers</fullName>
    <active>true</active>
    <description>The user can&apos;t enter negative numbers in the field</description>
    <errorConditionFormula>OCE__ColleagueAttendeeQuota__c &lt; 0  ||  OCE__EstimatedParticipants__c &lt; 0</errorConditionFormula>
    <errorDisplayField>OCE__ColleagueAttendeeQuota__c</errorDisplayField>
    <errorMessage>Invalid value, must be greater or equal to zero</errorMessage>
</ValidationRule>
